class Cat:
    property name = 'Dinah'
end

class Adventurer:
    property name = 'Alice'
    property cat
    
    method __init():
        self.cat = new Cat()
    end

    method imaginaryMethod():
        return new Cat()
    end
end

var adv = new Adventurer()
var ret_value = adv.imaginaryMethod?.().name
log(ret_value)